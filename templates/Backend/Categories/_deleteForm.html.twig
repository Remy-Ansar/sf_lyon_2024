<form action="{{ path('admin.categories.delete',{id: categorie.id}) }}" method="POST" onsubmit="return confirm('Êtes-vous certain de vous vouloir supprimer cette catégorie?')">
	<input type="hidden" name="token" value="{{ csrf_token('delete' ~ categorie.id) }}">
	<button class="btn btn-danger">
		<i class="bi bi-trash3-fill"></i>Supprimer</button>
</form>
